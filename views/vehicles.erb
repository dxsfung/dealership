<h1 class="text-center">Vehicles</h1>

<div class="container-fluid">
<li class="list-group-item">
    <img class="center-block img-responsive" style="max-height:300px" src="https://media.giphy.com/media/X5quY9aOi0cJa/giphy.gif
"></li>
</div>

  <h3 class="text-center">Here are the vehicles on this lot: </h3>

  <% @vehicles.each() do |vehicle| %>
    <% if vehicle != nil %>
        <form action="/vehicles/<%= vehicle.id() %>">
            <button class="btn btn-success btn-cons center-block">
                <%= vehicle.make %>
                <%= vehicle.model %>
                <%= vehicle.year %>
                <%= vehicle.color %>
            </button>
        </form>
    <% end %>
  <% end %>

  <form action="/">
      <button class="btn btn-primary center-block">
          Return to Main Page
      </button>
  </form>
