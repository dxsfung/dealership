<h1 class="text-center">Dealerships</h1>

<div class="container-fluid">
    <li class="list-group-item">
        <img class="center-block img-responsive" style="max-height:300px" src="https://media.giphy.com/media/X5quY9aOi0cJa/giphy.gif
    "></li>
</div>

<h3 class="text-center">Click one to view more:</h3>

<% @dealerships.each() do |dealership| %>
  <form action="/dealerships/<%= dealership.id %>">
      <button class="btn btn-success btn-cons center-block">
          <%= dealership.name() %>
      </button>
  </form>
<% end %>

<form action="/dealerships/new">
    <button class="btn btn-info center-block">
        Add New Dealership
    </button>
</form>

<form action="/">
    <button class="btn btn-primary center-block">
        Return to Main Page
    </button>
</form>
